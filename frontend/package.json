{
  "name": "hanko-frontend",
  "description": "hanko frontend parts",
  "private": true,
  "workspaces": [
    "frontend-sdk",
    "elements",
    "examples/angular",
    "examples/nextjs",
    "examples/react",
    "examples/vue",
    "examples/express"
  ],
  "scripts": {
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "start": "turbo run start",
    "build:elements": "turbo build --filter=./frontend-sdk --filter=./elements",
    "build:elements:dev": "turbo build:dev --filter=./elements"
  },
  "devDependencies": {
    "turbo": "^2.8.12"
  },
  "packageManager": "npm@11.6.2"
}
